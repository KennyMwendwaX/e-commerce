// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String     @id @default(cuid())
  name      String
  email     String     @unique
  password  String
  createdAt String
  products  Products[]
}

model Products {
  id          String   @id @default(cuid())
  name        String
  brand       String
  category    String
  description String
  price       Int
  quantity    Int
  imgUrl      String
  createdAt   DateTime
  User        User?    @relation(fields: [userId], references: [id])
  userId      String?
}
