{
  "name": "e-commerce",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "buildcss": "tailwindcss -i ./styles/input.css -o ./styles/global.css",
    "watchcss": "tailwindcss -i ./styles/input.css -o ./styles/global.css --watch",
    "json": "npx json-server ./src/asset/data.json --port 8000",
    "watchjson": "npx json-server ./src/asset/data.json --port 8000 --watch"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "daisyui": "^2.49.0",
    "json-server": "^0.17.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
